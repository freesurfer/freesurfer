set version = '$Id: morph_subject,v 1.4 2011/03/02 20:16:39 nicks Exp $';

if($#argv != 1) then
  echo "USAGE: morph_subject subjid"
  echo $version
  exit 1;
endif

set subjdir =  $SUBJECTS_DIR/$1
if(! -e $subjdir) then
  echo "ERROR: $subjdir does not exist"
  exit 1;
endif

morph_subject-lh $1
morph_subject-rh $1

exit 0;
