#!/bin/tcsh -ef

setenv GCA $MRI_DIR/average/mixed.gca
setenv LTA talairach.lta
setenv GCA $CMA/average/mixed_a1_ma.gca

setenv s $1

set sdir = $SUBJECTS_DIR/$s/mri
foreach d (fsamples norm transforms)
	mkdir -p $sdir/$d ;\
end


mri_em_register -fsamples $sdir/fsamples -norm $sdir/norm $sdir/orig $GCA $sdir/transforms/$LTA

