  cd $subjdir/mri > /dev/null
  if ($longitudinal) then
    # longitudinal processing:
    # just copy from the cross-sectionaly processed data for this subject
    set cmd = (cp -vf ${SUBJECTS_DIR}/${tpNid}/mri/nu.mgz .)
  else
